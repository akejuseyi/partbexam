data = ['1234', '123456', '12345678910', '123567',
        '013', '011', '854310', '12345', '1238974']

lista = []
listb = []
listc = []


def tuple_of_item(arg):
    x = 0
    while x < len(arg):
        if len(data[x]) <= 4:
            lista.append(arg[x])
        elif len(data[x]) >= 7:
            listb.append(arg[x])
        x = x+1
    y = (lista, listb)
    print(y)


tuple_of_item(data)
